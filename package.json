{
  "name": "whatspoppin",
  "version": "1.0.0",
  "description": "A starter template to build websites with Ghost & Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "test": "yarn run build",
    "lint": "eslint . --ext .js --cache"
  },

  "keywords": [
    "eleventy",
    "ghost",
    "starter",
    "boilerplate",
    "static",
    "site",
    "jamstack"
  ],
  "author": "David Darnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TryGhost/eleventy-starter-ghost/issues"
  },
  "homepage": "https://github.com/TryGhost/eleventy-starter-ghost#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@navillus/eleventy-plugin-inline-css": "^1.0.3",
    "@tryghost/content-api": "^1.2.5",
    "@tryghost/helpers": "^1.1.6",
    "aos": "^2.3.4",
    "autoprefixer": "^10.0.1",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "eleventy-plugin-lazyimages": "liamfiddler/eleventy-plugin-lazyimages",
    "eleventy-plugin-local-images": "^0.4.0",
    "eleventy-plugin-sass": "^1.1.1",
    "eslint-utils": "2.1.0",
    "html-minifier": "^4.0.0",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "reveal.js": "^4.0.2",
    "sass": "^1.26.11",
    "tailwindcss": "^1.8.10",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "eslint": "7.10.0",
    "eslint-plugin-ghost": "1.5.0"
  }
}
