---
pagination:
  data: collections.posts
  size: 6
  alias: posts
---

{% extends 'layouts/default.njk' %}

{% from "partials/card.njk" import card %}

{% set title = "Posts" %}

{% block content %}
<div class="container page">
    <section class="post-feed">
        {% for post in posts %}
            {{ card(post) }}
        {% endfor %}
    </section>
</div>
    {% include "partials/pagination.njk" %}
{% endblock%}
